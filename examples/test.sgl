fn gcd(a: int, b: int) -> int {
	if b == 0 { a } else { gcd(b, a % b) }
}

type Name {
	First(String),
	Last {
		first: String,
		last: String,
	}
}

fn add(a: int, b: int) -> int {
	a + b
}
fn sub(a: int, b: int) -> int {
	a - b
}

fn main() -> int {
	let a: int = 1;
	let b: int = 2;
	let c: int = if a > b { add } else { sub }(a, b)[123](a);
	let c: int = match gcd(a, b) {
		(a, b) => a,
		Name::Last { first: a, last: b } => b,
		Name::First(a) => a,
		Name::Last { first, second } => None,
	};
}

